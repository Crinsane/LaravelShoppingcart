{
  "name": "wolkers/laravel-shopping-cart",
  "description": "a Laravel shopping cart",
  "keywords": [
    "laravel",
    "shoppingcart"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rob Gloudemans",
      "email": "info@robgloudemans.nl"
    },
    {
      "name": "Thomas Vercruysse",
      "email": "thomas@wolkers.be"
    }
  ],
  "require": {
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
    "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0|~6.0|~7.0",
    "mockery/mockery": "~0.9.0",
    "orchestra/testbench": "~3.1"
  },
  "autoload": {
    "psr-4": {
      "Wolkers\\Shoppingcart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wolkers\\Tests\\Shoppingcart\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Wolkers\\Shoppingcart\\ShoppingcartServiceProvider"
      ],
      "aliases": {
        "Cart": "Wolkers\\Shoppingcart\\Facades\\Cart"
      }
    }
  }
}
